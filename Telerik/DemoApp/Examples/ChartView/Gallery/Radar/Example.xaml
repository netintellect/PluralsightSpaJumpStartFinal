                    <DataTemplate>
                        <Ellipse Fill="#9CD60F" Height="7" Width="7" />
                    </DataTemplate>
                </telerik:RadarLineSeries.PointTemplate>
            </telerik:RadarLineSeries>
            <telerik:RadarLineSeries Stroke="#25A0DB">
