            <Setter Property="FontFamily" Value="Segoe UI Light" />
            <Setter Property="FontSize" Value="16" />
            <Setter Property="Foreground" Value="{StaticResource MainBrush}" />
        </Style>

        <Style x:Key="DocumentHostTextBlockStyle" TargetType="TextBlock" BasedOn="{StaticResource HeaderTextBlockStyle}">
            <Setter Property="TextWrapping" Value="Wrap" />
            <Setter Property="FontSize" Value="24" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Margin" Value="8 10 20 0" />
        </Style>

        <Style x:Key="customGridStyle" TargetType="Grid">
            <Setter Property="Height" Value="150" />
            <Setter Property="Width" Value="205" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Background" Value="{StaticResource DarkGrayBrush}" />
        </Style>

        <Style x:Key="PaneLabelStyle" TargetType="TextBlock" BasedOn="{StaticResource HeaderTextBlockStyle}">
            <Setter Property="FontSize" Value="13" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Margin" Value="5 8 0 0" />
        </Style>
    </UserControl.Resources>
