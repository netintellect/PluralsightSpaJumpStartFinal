<UserControl x:Class="Telerik.Windows.Examples.Map.TrafficProvider.Example"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:example="clr-namespace:Telerik.Windows.Examples.Map.Shapefile"
    xmlns:telerikQuickStart="clr-namespace:Telerik.Windows.Controls.QuickStart;assembly=QuickStart.Common"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation">

    <Grid x:Name="LayoutRoot">

        <telerik:RadMap x:Name="RadMap1" Center="40.7254808779182,-73.9034722532031" ZoomLevel="11" 
                        MiniMapExpanderVisibility="Collapsed">
            <telerik:RadMap.Providers>
                <telerik:OpenStreetMapProvider />
                <telerik:BingMapTrafficProvider />
            </telerik:RadMap.Providers>
        </telerik:RadMap>

    </Grid>
</UserControl>
